country_stats <- list()
for (i in 1:nrow(input_freqs)) {
country_stats[[i]] <- c(length(list_top100[[i]]), length(list_top50[[i]]),
length(list_top20[[i]]), length(list_top10[[i]]))
}
names(country_stats) = country_vec
return (country_stats)
